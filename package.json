{
  "name": "ssr-app-api",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "format": "prettier src/**/*.{js,ts,tsx,scss,css,json}",
    "format:fix": "prettier --write src/**/*.{js,ts,tsx,scss,css,json}",
    "lint": "eslint src/ --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint src/ --ext js,ts,tsx --fix",
    "lint:staged": "lint-staged",
    "test": "npx vitest src/ --run --coverage",
    "test:run": "cls && vitest src/ --run",
    "test:coverage": "npx vitest src/ --coverage",
    "prepare": "husky install",
    "pp": "npm run format:fix && npm run lint:fix && npm run test"
  },
  "lint-staged": {
    "src/**/*.{js,ts,tsx,scss,css,json}": "prettier --write",
    "src/**/*.{js,ts,tsx}": "eslint --fix --max-warnings=0"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^2.2.6",
    "next": "^14.2.5",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-redux": "^9.1.2",
    "swr": "^2.2.5"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.3.0",
    "@commitlint/config-conventional": "^19.2.2",
    "@eslint/js": "^9.6.0",
    "@next/eslint-plugin-next": "^14.2.5",
    "@testing-library/jest-dom": "^6.4.6",
    "@testing-library/react": "^16.0.0",
    "@testing-library/user-event": "^14.5.2",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@typescript-eslint/eslint-plugin": "^7.15.0",
    "@typescript-eslint/parser": "^7.15.0",
    "@vitejs/plugin-react": "^4.3.1",
    "@vitest/coverage-v8": "^1.6.0",
    "@vitest/ui": "^1.6.0",
    "eslint": "^8.57.0",
    "eslint-config-next": "^14.2.5",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-prettier": "^5.1.3",
    "globals": "^15.8.0",
    "husky": "^9.0.11",
    "jsdom": "^24.1.0",
    "lint-staged": "^15.2.7",
    "msw": "^2.3.2",
    "next-router-mock": "^0.9.13",
    "prettier": "^3.3.2",
    "typescript": "^5.2.2",
    "typescript-eslint": "^7.15.0",
    "vite": "^5.3.1",
    "vitest": "^1.6.0"
  }
}
