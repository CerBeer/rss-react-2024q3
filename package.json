{
  "name": "remix-ssr",
  "private": true,
  "sideEffects": false,
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "build": "remix vite:build",
    "dev": "remix vite:dev",
    "start": "remix-serve build/server/index.js",
    "typecheck": "tsc",
    "format": "prettier app/**/*.{js,ts,tsx,scss,css,json}",
    "format:fix": "prettier --write app/**/*.{js,ts,tsx,scss,css,json}",
    "lint": "eslint --ignore-path .gitignore --cache --cache-location ./node_modules/.cache/eslint .",
    "lint:fix": "eslint . --ext js,ts,tsx --fix",
    "lint:staged": "lint-staged",
    "test": "npx vitest run --coverage",
    "test:run": "cls && vitest run",
    "test:coverage": "npx vitest --coverage",
    "prepare": "husky install",
    "pp": "npm run format:fix && npm run lint:fix && npm run test"
  },
  "lint-staged": {
    "src/**/*.{js,ts,tsx,scss,css,json}": "prettier --write",
    "src/**/*.{js,ts,tsx}": "eslint --fix --max-warnings=0"
  },
  "dependencies": {
    "@remix-run/node": "^2.11.1",
    "@remix-run/react": "^2.11.1",
    "@remix-run/serve": "^2.11.1",
    "@remix-run/testing": "^2.11.1",
    "isbot": "^4.1.0",
    "match-sorter": "^6.3.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "sort-by": "^1.2.0",
    "tiny-invariant": "^1.3.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.3.0",
    "@commitlint/config-conventional": "^19.2.2",
    "@eslint/js": "^9.6.0",
    "@remix-run/dev": "^2.11.1",
    "@testing-library/jest-dom": "^6.4.6",
    "@testing-library/react": "^16.0.0",
    "@testing-library/user-event": "^14.5.2",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@typescript-eslint/eslint-plugin": "^7.15.0",
    "@typescript-eslint/parser": "^7.15.0",
    "@vitejs/plugin-react": "^4.3.1",
    "@vitest/coverage-v8": "^1.6.0",
    "@vitest/ui": "^1.6.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-hooks": "^4.6.2",
    "globals": "^15.8.0",
    "husky": "^9.0.11",
    "jsdom": "^24.1.0",
    "lint-staged": "^15.2.7",
    "msw": "^2.3.2",
    "prettier": "^3.3.2",
    "typescript": "^5.2.2",
    "typescript-eslint": "^7.15.0",
    "vite": "^5.3.1",
    "vite-tsconfig-paths": "^4.3.1",
    "vitest": "^1.6.0"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
